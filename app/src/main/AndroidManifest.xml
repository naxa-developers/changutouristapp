            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.naxa.np.changunarayantouristapp.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity android:name="com.naxa.np.changunarayantouristapp.splashscreen.WalkThroughSliderActivity" />
